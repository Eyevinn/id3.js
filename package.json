{
  "name": "simple-id3.js",
  "version": "0.1.0",
  "description": "Simple ID3 parser library in Javascript",
  "main": "id3.js",
  "repository": "https://github.com/Eyevinn/id3.js",
  "scripts": {
    "clean": "rm dist/*",
    "prebuild": "npm run clean && npm run test",
    "build": "browserify id3.js --standalone ID3 > dist/id3.js",
    "postbuild": "npm run minify",
    "minify": "node_modules/uglify-js/bin/uglifyjs dist/id3.js > dist/id3.min.js",
    "pretest": "npm run lint",
    "lint": "node_modules/jshint/bin/jshint id3.js",
    "test": "node_modules/karma/bin/karma start karma.conf.js"
  },
  "author": "Jonas Birm√© <jonas.birme@eyevinn.se> (http://www.eyevinntechnology.se)",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^13.1.0",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "jshint": "^2.9.3",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "uglify-js": "^2.7.3",
    "watchify": "^3.7.0"
  }
}
